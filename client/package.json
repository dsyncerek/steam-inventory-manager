{
  "name": "steam-inventory-manager-client",
  "version": "1.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build --configuration production",
    "analyze": "npm run build -- --stats-json && npx webpack-bundle-analyzer dist/stats.json",
    "format": "prettier src/**/*.{ts,scss,html} --write",
    "lint": "npm run lint:scripts && npm run lint:styles",
    "lint:scripts": "eslint src/**/*.ts --fix",
    "lint:styles": "stylelint src/**/*.scss --fix",
    "precommit": "lint-staged"
  },
  "dependencies": {
    "@angular/animations": "^12.0.1",
    "@angular/cdk": "^15.2.8",
    "@angular/common": "^12.0.1",
    "@angular/compiler": "^15.2.8",
    "@angular/core": "^12.0.1",
    "@angular/flex-layout": "^12.0.0-beta.34",
    "@angular/forms": "^12.0.1",
    "@angular/material": "^15.2.8",
    "@angular/platform-browser": "^12.0.1",
    "@angular/platform-browser-dynamic": "^12.0.1",
    "@angular/router": "^12.0.1",
    "@ngrx/effects": "^12.0.0",
    "@ngrx/entity": "^12.0.0",
    "@ngrx/store": "^12.5.1",
    "@ngrx/store-devtools": "^12.0.0",
    "normalizr": "^3.6.2",
    "rxjs": "^6.6.7",
    "tslib": "^2.2.0",
    "zone.js": "~0.11.8"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^12.0.1",
    "@angular/cli": "^15.2.6",
    "@angular/compiler-cli": "^12.0.1",
    "@ngrx/schematics": "^15.4.0",
    "@typescript-eslint/eslint-plugin": "^4.33.0",
    "@typescript-eslint/parser": "^4.33.0",
    "eslint": "^7.27.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-config-standard-with-typescript": "^21.0.1",
    "eslint-plugin-import": "^2.23.3",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-standard": "^5.0.0",
    "lint-staged": "^13.2.1",
    "prettier": "^2.3.0",
    "stylelint": "^15.6.0",
    "stylelint-config-prettier": "^9.0.5",
    "stylelint-config-standard": "^33.0.0",
    "typescript": "~4.3.5"
  }
}
