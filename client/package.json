{
  "name": "steam-inventory-manager-client",
  "version": "1.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build --prod",
    "analyze": "npm run build -- --stats-json && npx webpack-bundle-analyzer dist/stats.json",
    "format": "prettier src/**/*.{ts,scss,html} --write",
    "lint": "npm run lint:scripts && npm run lint:styles",
    "lint:scripts": "eslint src/**/*.ts --fix",
    "lint:styles": "stylelint src/**/*.scss --fix",
    "precommit": "lint-staged"
  },
  "dependencies": {
    "@angular/animations": "^11.0.2",
    "@angular/cdk": "^11.0.0",
    "@angular/common": "^11.0.2",
    "@angular/compiler": "^11.0.2",
    "@angular/core": "^11.0.2",
    "@angular/flex-layout": "^11.0.0-beta.33",
    "@angular/forms": "^11.0.2",
    "@angular/material": "^11.0.0",
    "@angular/platform-browser": "^11.0.2",
    "@angular/platform-browser-dynamic": "^11.0.2",
    "@angular/router": "^11.0.2",
    "@ngrx/effects": "^10.0.1",
    "@ngrx/entity": "^10.0.1",
    "@ngrx/store": "^10.0.1",
    "@ngrx/store-devtools": "^10.0.1",
    "normalizr": "^3.6.1",
    "rxjs": "^6.6.3",
    "tslib": "^2.1.0",
    "zone.js": "~0.10.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.1100.6",
    "@angular/cli": "^11.2.0",
    "@angular/compiler-cli": "^11.0.2",
    "@ngrx/schematics": "^10.1.2",
    "@typescript-eslint/eslint-plugin": "^3.10.1",
    "@typescript-eslint/parser": "^3.10.1",
    "eslint": "^7.19.0",
    "eslint-config-prettier": "^7.2.0",
    "eslint-config-standard-with-typescript": "^18.0.2",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^5.0.0",
    "lint-staged": "^10.5.4",
    "prettier": "^2.2.1",
    "stylelint": "^13.10.0",
    "stylelint-config-prettier": "^8.0.2",
    "stylelint-config-standard": "^20.0.0",
    "typescript": "~4.0.5"
  }
}
